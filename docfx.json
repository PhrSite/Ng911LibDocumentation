{
  "metadata": [
    {
      "src": [
        {
            "files": [ "**/*.csproj" ],
            "exclude": [ "**/bin/**", "**/obj/**" ],
            "src": "../Ng911Lib/ClassLibrary"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/",
                    "articles/toc.yml",
                    "*.md",
                    "*.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "docs",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "statictoc"
        ],
        "postProcessors": [ "ExtractSearchIndex" ],
        "globalMetadata": {
            "_enableSearch": "true"
        },
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}